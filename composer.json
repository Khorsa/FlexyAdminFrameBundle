{
  "name": "flexycms/flexyadminframe-bundle",
  "version": "0.1.4.4",
  "type": "symfony-bundle",
  "description": "Main frame for admin panel",
  "license": "proprietary",
  "require": {
    "php": "^7.3",
    "phpmailer/phpmailer": "^6.1",
    "tinymce/tinymce": "^5",
	"symfony/webpack-encore-bundle": "*",
	"twbs/bootstrap": "4.5.3",
	"moment/moment": "*"
  },
  "require-dev": {
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "flexycms\\FlexyAdminFrameBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flexycms\\FlexyAdminFrameBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test" : "./vendor/bin/simple-phpunit"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.0.*"
    }
  }
}